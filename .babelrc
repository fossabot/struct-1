{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "last 2 Chrome versions"
        }
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": false }],
    ["@babel/plugin-proposal-private-methods", { "loose": false }]
  ]
}
